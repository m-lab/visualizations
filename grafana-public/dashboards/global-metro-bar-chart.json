          "editorMode": "code",
          "format": 1,
          "location": "",
          "project": "mlab-oti",
          "rawQuery": true,
          "rawSql": "SELECT\n  name,\n  KSdistance*10 AS scaledKSdistance,\n  IF (Sites != '1', spread, 0) AS Spread,\n  Continent,\n  metro,\nFROM `${dataset}.cached_metro_report`(\"MinRTT\", \"${ChartMetros:regex}\", ${ISPcount})\n  JOIN (\n    SELECT\n      REGEXP_EXTRACT(site, '^([a-z]{3})') AS metro,\n      ANY_VALUE(FORMAT('%t %t (%t)', City, CountryCode,  REGEXP_EXTRACT(site, '^([a-z]{3})'))) AS name,\n      ANY_VALUE(ContinentCode) AS Continent,\n      ANY_VALUE(FORMAT('%t %t %t', ContinentCode, CountryCode , City )) AS sortKey, \n    FROM `${dataset}.cached_metadata`\n    GROUP BY metro\n  ) USING (metro)\nWHERE level like 'Metro%%'\n  AND (\"${verbose}\" = 'yes' OR Sites != '1')\nORDER BY sortKey\n",
          "refId": "A",
          "sql": {
            "columns": [
              {
                "parameters": [],
                "type": "function"
              }
            ],
            "groupBy": [
              {
